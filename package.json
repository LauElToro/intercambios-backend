{
  "name": "intercambius-backend",
  "version": "1.0.0",
  "description": "Backend API para Intercambius con arquitectura DDD",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/infrastructure/server.ts",
    "build": "tsc && prisma generate",
    "start": "node dist/infrastructure/server.js",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/infrastructure/database/seed.ts",
    "db:check-coords": "tsx scripts/check-and-backfill-coords.ts",
    "vercel-build": "node scripts/vercel-build.cjs"
  },
  "keywords": [
    "intercambius",
    "api",
    "backend",
    "ddd"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@react-google-maps/api": "^2.20.8",
    "@types/leaflet": "^1.9.21",
    "@vercel/blob": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^8.0.1",
    "react-leaflet": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^7.0.10",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
